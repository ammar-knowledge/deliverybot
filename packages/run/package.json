{
  "name": "@deliverybot/run",
  "version": "0.7.2",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/*",
    "bundle/*",
    "config/*"
  ],
  "scripts": {
    "clean": "mkdir -p lib && mkdir -p bundle && rm -r lib && rm -r bundle",
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "bundle": "webpack --config config/webpack.config.js",
    "bundle:watch": "webpack --config config/webpack.config.js --watch",
    "run": "node lib/index.js"
  },
  "dependencies": {
    "@deliverybot/app": "0.7.2",
    "@deliverybot/client": "0.7.2",
    "@deliverybot/core": "0.7.2",
    "@google-cloud/pubsub": "1.7.3",
    "@octokit/app": "14.0.2",
    "@octokit/webhooks": "12.0.11",
    "apexcharts": "3.45.2",
    "dotenv": "8.2.0",
    "express": "4.20.0",
    "firebase": "10.7.2",
    "firebase-admin": "12.0.0",
    "firebase-functions": "3.24.1",
    "firebase-tools": "13.6.0",
    "hbs": "4.2.0"
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "typescript": "3.7.2",
    "webpack": "5.96.1",
    "webpack-cli": "^3.3.12"
  },
  "description": "Controls and configures deployments using the GitHub API.",
  "author": "Deliverybot <support@deliverybot.dev> (https://github.com/deliverybot/deliverybot)",
  "license": "MIT",
  "support": "support@deliverybot.dev",
  "repository": "https://github.com/deliverybot/deliverybot",
  "homepage": "https://deliverybot.dev",
  "documentation": "https://deliverybot.dev/docs/",
  "bugs": "https://github.com/deliverybot/deliverybot/issues",
  "community": "https://spectrum.chat/deliverybot/general",
  "github": "https://github.com/deliverybot",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ]
}
